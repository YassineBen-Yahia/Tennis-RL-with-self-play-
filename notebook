import os\nimport gym\nimport numpy as np\nimport supersuit\nfrom stable_baselines3 import PPO\nfrom stable_baselines3.common.vec_env import DummyVecEnv\nfrom pettingzoo.atari import tennis_v3\nfrom collections import deque\nfrom supersuit import pettingzoo_env_to_vec_env_v1, concat_vec_envs_v1\nfrom pettingzoo.utils.conversions import aec_to_parallel\nfrom pettingzoo.utils import AECEnv\nfrom pettingzoo.utils.wrappers import BaseParallelWrapper","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-09-09T17:18:13.378363Z","iopub.execute_input":"2025-09-09T17:18:13.378616Z","iopub.status.idle":"2025-09-09T17:18:13.383973Z","shell.execute_reply.started":"2025-09-09T17:18:13.378599Z","shell.execute_reply":"2025-09-09T17:18:13.383223Z"}},"outputs":[],"execution_count":56},{"cell_type":"code","source":"class SelfPlayWrapper(BaseParallelWrapper):\n    def __init__(self, env, opponent_policy):\n        super().__init__(env)\n        self.env = env\n        self.opponent_policy = opponent_policy\n        # Select agents\n        self.agents = self.env.possible_agents\n        self.learning_agent = self.env.possible_agents[0]\n        self.opponent_agent = self.env.possible_agents[1]\n        # Gym-compatible spaces for the learning agent\n        self.observation_space = self.env.observation_space\n        self.action_space = self.env.action_space\n    \n    def reset(self, **kwargs):\n        return self.env.reset(**kwargs)\n        \n    \n    def step(self, actions):\n        # Convert single action to actions dict for both agents\n        if not isinstance(actions, dict):\n            # This means we got a single action from the learning agent\n            learning_action = actions\n        else:\n            learning_action = actions.get(self.learning_agent, 0)\n        \n        # Get opponent action if opponent is alive\n        if self.opponent_agent in self.env.agents:\n            opp_obs = self.env.observe(self.opponent_agent)\n            opp_action, _ = self.opponent_policy.predict(opp_obs, deterministic=True)\n        else:\n            opp_action = 0  # Default action for dead agent\n        \n        actions_dict = {\n            self.learning_agent: learning_action,\n            self.opponent_agent: opp_action\n        }\n        \n        # Step the environment\n        self.env.step(actions_dict)\n        \n        # Collect observations, rewards, terminations, truncations, infos for all agents\n        observations = {}\n        rewards = {}\n        terminations = {}\n        truncations = {}\n        infos = {}\n        \n        for agent in self.env.possible_agents:\n            if agent in self.env.agents:\n                observations[agent] = self.env.observe(agent)\n                rewards[agent] = self.env.rewards.get(agent, 0)\n                terminations[agent] = False  # Agent is still alive\n                truncations[agent] = False\n                infos[agent] = self.env.infos.get(agent, {}) if hasattr(self.env, 'infos') else {}\n            else:\n                # Dead agent - will be handled by black_death wrapper\n                observations[agent] = None\n                rewards[agent] = 0\n                terminations[agent] = True\n                truncations[agent] = False\n                infos[agent] = {}\n        \n        return observations, rewards, terminations, truncations, infos\n    \n    def set_opponent(self, opponent_policy):\n        self.opponent_policy = opponent_policy\n\ndef make_env(opponent_policy, render_mode=None):\n    def _init():\n        base_env = tennis_v3.env(render_mode=render_mode or \"rgb_array\")\n        env = SelfPlayWrapper(base_env, opponent_policy)\n        env=supersuit.black_death_v3(env)\n        env = pettingzoo_env_to_vec_env_v1(env)\n        return env\n    return _init","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-09-09T18:22:23.393251Z","iopub.execute_input":"2025-09-09T18:22:23.393950Z","iopub.status.idle":"20