027881Z"}},"outputs":[],"execution_count":11},{"cell_type":"code","source":"import gym\nfrom collections import deque\n\nclass SelfPlayWrapper(gym.Env):\n    def __init__(self, base_env, opponent_policy):\n        super().__init__()\n        self.base_env = base_env\n        self.opponent_policy = opponent_policy\n        self.agents = self.base_env.possible_agents\n        self.learning_agent = self.agents[0]\n        self.opponent_agent = self.agents[1]\n\n        self.observation_space = self.base_env.observation_space(self.learning_agent)\n        self.action_space = self.base_env.action_space(self.learning_agent)\n\n    def reset(self, **kwargs):\n        obs, infos = self.base_env.reset(**kwargs)\n        return obs[self.learning_agent]\n\n    def step(self, action):\n        # Get opponent observation\n        opp_obs = self.base_env.observe(self.opponent_agent)\n\n        # Opponent picks action\n        opp_action, _ = self.opponent_policy.predict(opp_obs, deterministic=True)\n\n        actions = {\n            self.learning_agent: action,\n            self.opponent_agent: opp_action\n        }\n\n        obs, rewards, terminations, truncations, infos = self.base_env.step(actions)\n        done = terminations[self.learning_agent] or truncations[self.learning_agent]\n\n        return (\n            obs[self.learning_agent],\n            rewards[self.learning_agent],\n            done,\n            infos[self.learning_agent],\n        )\n\n    def set_opponent(self, opponent_policy):\n        self.opponent_policy = opponent_policy\n\n\n\ndef make_env(opponent_policy, render_mode=None):\n    def _init():\n        base_env = tennis_v3.env(render_mode=render_mode)\n        env = SelfPlayWrapper(base_env, opponent_policy)\n        env = pettingzoo_env_to_vec_env_v1(env)\n        env = concat_vec_envs_v1(env, 1, num_cpus=0, base_class=\"stable_baselines3\")\n        return env\n    return _init\n\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025